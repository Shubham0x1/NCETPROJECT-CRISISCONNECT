# .env.example

# MongoDB Database Connection
MONGO_URI=your_mongodb_connection_string_goes_here

# JSON Web Token Secret
JWT_SECRET=a_long_random_secret_string_goes_here

# Port for the server to run on
PORT=5000

# Cloudinary Credentials for Image Upload
CLOUDINARY_CLOUD_NAME=your_cloud_name_goes_here
CLOUDINARY_API_KEY=your_api_key_goes_here
CLOUDINARY_API_SECRET=your_api_secret_goes_here